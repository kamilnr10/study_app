{
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "import/first": 0,
        "prettier/prettier": ["error", {"singleQuote": true}]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint"
  }